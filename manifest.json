{
    "manifest_version": 3,
    "name": "Color Picker Extension",
    "version": "1.0",
    "description": "A color picker extension that provides hexcode of selected color",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/picker_16.png",
        "32": "images/picker_32.png",
        "48": "images/picker_48.png"
      }
    },
    "icons": {
      "16": "images/picker_16.png",
      "32": "images/picker_32.png",
      "48": "images/picker_48.png"
    }
  }
  